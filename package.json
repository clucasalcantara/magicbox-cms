{
  "name": "effect-cms",
  "version": "1.0.0",
  "description": "A CMS developed to be magic, and provides you a quickly, automated and innovative interface prototyping, build and deploy proccess",
  "main": "index.js",
  "scripts": {
    "build-server": "webpack --display=errors-only --config webpack/webpack.server.js --watch",
    "build-client": "webpack --display=errors-only --config webpack/webpack.client.js --watch",
    "dev:server": "nodemon --watch build --exec \"node build/bundle.js\"",
    "dev": "npm-run-all --parallel build-* dev:*",
    "graphql": "node ./scripts/start.js",
    "test": "node scripts/test.js --env=jsdom",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "author": "",
  "license": "ISC",
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/jest/setup.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "dependencies": {
    "apisauce": "^1.1.1",
    "auth0-js": "^9.3.3",
    "axios": "0.19.2",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-jest": "26.0.1",
    "babel-loader": "8.1.0",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-es2017": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "babel-register": "6.26.0",
    "bluebird": "3.7.2",
    "body-parser": "1.19.0",
    "chalk": "4.1.0",
    "compression": "^1.7.3",
    "concurrently": "5.2.0",
    "dotenv": "8.2.0",
    "dotenv-expand": "5.1.0",
    "dotenv-safe": "8.2.0",
    "express": "^4.16.4",
    "express-graphql": "0.9.0",
    "express-http-proxy": "1.6.0",
    "graphql": "15.1.0",
    "jest": "26.0.1",
    "jsdoc": "^3.5.5",
    "lodash": "^4.17.19",
    "mongodb": "3.5.9",
    "nodemon": "^2.0.4",
    "npm-run-all": "4.1.5",
    "piping": "1.0.0-rc.4",
    "pretty-format": "26.0.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-helmet": "^6.1.0",
    "react-redux": "7.2.0",
    "react-router-config": "5.1.1",
    "react-router-dom": "5.2.0",
    "redux": "4.0.5",
    "redux-saga": "^1.1.3",
    "serialize-javascript": "4.0.0",
    "webpack": "4.43.0",
    "webpack-dev-server": "^3.1.10",
    "webpack-merge": "4.2.2",
    "webpack-node-externals": "1.7.2"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^5.3.19",
    "@storybook/addon-links": "^5.3.19",
    "@storybook/addons": "^5.3.19",
    "@storybook/react": "^5.3.19",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-preset-flow": "^6.23.0",
    "enzyme": "^3.3.0",
    "flow-bin": "^0.127.0",
    "webpack-messages": "^2.0.4"
  }
}
